<?xml version="1.0" encoding="windows-1252"?>
<MyoVision_analysis>
	<raw_image>
		<image_file_string>..\demos\raw_images\power_3_gastroc_10x_blue_cropped.png</image_file_string>
	</raw_image>

	<image_to_label_parameters>
		<verbose_mode>1</verbose_mode>
		<saturation_percent>15</saturation_percent>
		<min_object_size>50</min_object_size>
		<block_size>1000</block_size>
		<block_image_file_string>..\demos\output\blocks\block</block_image_file_string>
	</image_to_label_parameters>
	
	<classification_parameters>
		<classification_file_string>..\demos\output\classification\classification_model.svc</classification_file_string>
		<verbose_mode>1</verbose_mode>
		<watershed_distance>10</watershed_distance>
		<classification_steps_image_file_string>..\demos\output\classification_steps\classification_steps.png</classification_steps_image_file_string>
	</classification_parameters>
	
	<refine_fibers_parameters>
		<max_iterations>25</max_iterations>
		<lambda2>1.5</lambda2>
		<refine_fibers_image_file_string>..\demos\output\refine_fibers\refine_fibers.png</refine_fibers_image_file_string>
	</refine_fibers_parameters>
	
	<results_parameters>
		<overlay_image_file_string>..\demos\output\final_results\final_overlay.png</overlay_image_file_string>
		<output_excel_file_string>..\demos\output\final_results\final_results.xlsx</output_excel_file_string>
	</results_parameters>

</MyoVision_analysis>